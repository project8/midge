    {
        "name" : "signal_data"
    },

    "rf_data" :
    {
        "name" : "wigner_data"
    },

    "rf_data" :
    {
        "name" : "power_data"
    },

    "rt_monarch_producer" :
    {
        "name" : "signal",
        "file" : "trap_on.egg",
        "minimum_time" : 0.123,
        "maximum_time" : 0.128,
        "size" : 32768,
        "stride" : 1024
    },

    "rt_rf_wigner_transformer" :
    {
        "name" : "wigner",
        "offset" : 16384,
        "length" : 16383,
        "window_hanning" :
        {
        }
    },

    "rt_rf_power_transformer" :
    {
        "name" : "power",
        "impedance_ohm" : 50,
        "window_rectangular" :
        {
        }
    },

    "rf_root_consumer" :
    {
        "name" : "wigner_output",
        "frequency_minimum" : 50.0e6,
        "frequency_maximum" : 56.0e6,
        "file" : "trap_wigner.root",
        "plot" : true,
        "plot_key" : "wigner_canvas",
        "plot_name" : "wigner_histogram",
        "chart_title" : "Wigner-Ville Distribution",
        "axis_title" : "Power [arb]"
    },

    "rf_root_consumer" :
    {
        "name" : "power_output",
        "frequency_minimum" : 50.0e6,
        "frequency_maximum" : 56.0e6,
